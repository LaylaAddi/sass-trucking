<p id="notice"><%= notice %></p>

<h1>Shipping Companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Telephone</th>
      <th>Website</th>
      <th>Logo</th>
      <th>Broker mc</th>
      <th>Carrier mc</th>
      <th>Us dot</th>
      <th>Shipper</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipping_companies.each do |shipping_company| %>
      <tr>
        <td><%= shipping_company.name %></td>
        <td><%= shipping_company.street %></td>
        <td><%= shipping_company.city %></td>
        <td><%= shipping_company.state %></td>
        <td><%= shipping_company.zip %></td>
        <td><%= shipping_company.telephone %></td>
        <td><%= shipping_company.website %></td>
        <td><%= shipping_company.logo %></td>
        <td><%= shipping_company.broker_mc %></td>
        <td><%= shipping_company.carrier_mc %></td>
        <td><%= shipping_company.us_dot %></td>
        <td><%= shipping_company.shipper_id %></td>
        <td><%= link_to 'Show', shipping_company %></td>
        <td><%= link_to 'Edit', edit_shipping_company_path(shipping_company) %></td>
        <td><%= link_to 'Destroy', shipping_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipping Company', new_shipping_company_path %>
