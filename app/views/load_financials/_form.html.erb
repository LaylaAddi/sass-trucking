<%= simple_form_for(@load_financial) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :gross_rate %>
    <%= f.input :percent_deducted %>
    <%= f.input :miles %>
    <%= f.input :gross_rpm %>
    <%= f.input :dollars_deducted_based_on_percent %>
    <%= f.input :gross_rate_after_percent %>
    <%= f.input :after_percent_deducted_rpm %>
    <%= f.input :load_expense %>
    <%= f.input :net_load_rate %>
    <%= f.input :net_rpm %>
    <%= f.input :load_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
