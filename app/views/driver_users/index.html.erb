<h1>All Drivers</h1>
<table class="table table-bordered table-striped table-condensed table-responsive">
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Company</th>
			<th>Type</th>
			<th>Employment Status</th>
			<th>[HEADER]</th>
			<th>[HEADER]</th>
		</tr>
	</thead>
	<tbody>
	  <% @drivers.each do |driver| %>
		<tr>
			<td><%= driver.id %></td>
			
			<td><%= link_to driver_user_path(driver) do %>
			  <%= driver.full_name %>
			  <% end %>
			</td>
			
			<td>text in td tag</td>
			<td><%= driver.type_of_user %></td>
			<td><%= driver.employment_status %></td> 
			<td>[CONTENT]</td>
			<td>[CONTENT]</td>
		</tr>
		<% end %>
	</tbody>
</table>
