<%= simple_form_for(@company) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %> 
    <%= f.input :address %>
    <%= f.input :telephone %>
    <%= f.input :website %>
    <%= f.input :logo %>
    <%= f.input :broker_mc_number %>
    <%= f.input :carrier_mc_number %>
    <%= f.input :us_dot_number %>
    <%= f.input :company_manager_id %>
    <%= f.input :company_type %>
    <%= f.input :date_formed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
