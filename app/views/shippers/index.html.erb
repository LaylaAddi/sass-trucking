<p id="notice"><%= notice %></p>

<h1>Shippers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Handle</th>
      <th>Contact person</th>
      <th>Telephone</th>
      <th>Fax</th>
      <th>Location</th>
      <th>Broker mc</th>
      <th>Carrier mc</th>
      <th>Dot number</th>
      <th>Website</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shippers.each do |shipper| %>
      <tr>
        <td><%= shipper.name %></td>
        <td><%= shipper.handle %></td>
        <td><%= shipper.contact_person %></td>
        <td><%= shipper.telephone %></td>
        <td><%= shipper.fax %></td>
        <td><%= shipper.location %></td>
        <td><%= shipper.broker_mc %></td>
        <td><%= shipper.carrier_mc %></td>
        <td><%= shipper.dot_number %></td>
        <td><%= shipper.website %></td>
        <td><%= shipper.street %></td>
        <td><%= shipper.city %></td>
        <td><%= shipper.state %></td>
        <td><%= shipper.zip %></td>
        <td><%= shipper.notes %></td>
        <td><%= link_to 'Show', shipper %></td>
        <td><%= link_to 'Edit', edit_shipper_path(shipper) %></td>
        <td><%= link_to 'Destroy', shipper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipper', new_shipper_path %>
