<p id="notice"><%= notice %></p>

<h1>Transactions</h1>

<table>
  <thead>
    <tr>
      <th>Week pay</th>
      <th>Escrow pay</th>
      <th>Trans dec 1</th>
      <th>Trans dec 2</th>
      <th>Trans dec 3</th>
      <th>Trans int 1</th>
      <th>Trans int 2</th>
      <th>Trans str 1</th>
      <th>Trans str 2</th>
      <th>Trans str 3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transactions.each do |transaction| %>
      <tr>
        <td><%= transaction.week_pay %></td>
        <td><%= transaction.escrow_pay %></td>
        <td><%= transaction.trans_dec_1 %></td>
        <td><%= transaction.trans_dec_2 %></td>
        <td><%= transaction.trans_dec_3 %></td>
        <td><%= transaction.trans_int_1 %></td>
        <td><%= transaction.trans_int_2 %></td>
        <td><%= transaction.trans_str_1 %></td>
        <td><%= transaction.trans_str_2 %></td>
        <td><%= transaction.trans_str_3 %></td>
        <td><%= link_to 'Show', transaction %></td>
        <td><%= link_to 'Edit', edit_transaction_path(transaction) %></td>
        <td><%= link_to 'Destroy', transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transaction', new_transaction_path %>
